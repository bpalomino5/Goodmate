{
  "env": { 
      "node": true,
      "browser": true,
      "es6": true
    },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": { 
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-use-before-define": ["error", { "variables": false }],
      "react/prop-types": 0,
      "arrow-parens": 0,
      "no-console": 0
    }
}
